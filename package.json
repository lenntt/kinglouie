{
  "name": "kinglouie-cypress",
  "version": "1.0.0",
  "description": "Cypress model-based monkey tester",
  "main": "index.js",
  "author": "Lennart Tange",
  "license": "MIT",
  "dependencies": {
    "cypress": "8.0.0",
    "cypress-axe": "^0.12.2",
    "axe-core": "^4.3.1"
  },
  "devDependencies": {
    "@types/chai": "^4.2.21",
    "@types/mocha": "^8.2.3",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "chai": "^4.3.4",
    "eslint": "^7.31.0",
    "mocha": "^9.0.2",
    "nyc": "^15.1.0",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
  },
  "scripts": {
    "build": "yarn run lint && yarn run test",
    "test": "mocha -r ts-node/register 'tests/**/*.ts'",
    "test:coverage": "nyc --reporter=html --reporter=text --reporter=text-summary yarn test",
    "lint": "eslint . --ext .ts",
    "watch": "TS_NODE_IGNORE_WARNINGS=TRUE mocha -r ts-node/register 'tests/**/*.ts'  --watch-files **/*.ts --watch"
  }
}
